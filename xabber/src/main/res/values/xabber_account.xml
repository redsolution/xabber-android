    <string name="tutorial_title_1">Welcome to Xabber</string>
    <string name="tutorial_title_2">Open Standards</string>
    <string name="tutorial_title_3">Rich Multimedia</string>
    <string name="tutorial_title_4">Privacy and Security</string>
    <string name="tutorial_title_5">Multiple Platforms</string>
    <string name="tutorial_title_6">Perfectly in Sync</string>
